{
	"name" : "dicr/php-dao",
	"description" : "Simple Dao for PHP",
	"type" : "library",
	"authors" : [{
			"name" : "Igor Tarasov",
			"email" : "develop@dicr.org",
			"homepage" : "http://dicr.org",
			"role" : "developer"
		}
	],
	"minimum-stability" : "stable",
	"autoload" : {
		"psr-4" : {
			"dicr\\dao\\" : "src"
		}
