	},
	"repositories" : [{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		}, {
			"type" : "git",
			"url" : "https://gitlab.com/dicr/php-dao.git"
		}
	],
	"homepage" : "https://gitlab.com/dicr/php-dao",
	"support" : {
		"source" : "https://gitlab.com/dicr/php-dao.git"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~8.1"
	},
	"autoload-dev" : {
		"psr-4" : {
			"\\dicr\\dao\\tests\\" : "tests"
		}
	}
}